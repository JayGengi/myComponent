<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.jaygengi.scenic">

    <application
        android:name=".runalone.application.ScenicApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:label,android:theme,android:name">
        <activity android:name=".ScenicActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Mai1nActivity"
            android:label="@string/title_activity_mai1n"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>